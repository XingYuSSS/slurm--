{
  "name": "slurm--extension",
  "displayName": "slurm--",
  "markdownDescription": "A multifunctional tool for SLURM syntax highlight, manage tasks, view resource, and launch tasks.",
  "version": "0.6.1",
  "icon": "assets/icons/extension-icon.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages",
    "Data Science",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "extensionKind": [
    "workspace"
  ],
  "publisher": "xy-sss",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "slurm--.refreshUserTasks",
        "title": "%slurm--.refreshUserTasks.title%",
        "category": "slurm--",
        "icon": "$(refresh)"
      },
      {
        "command": "slurm--.autoRefreshTask",
        "title": "%slurm--.autoRefreshTask.title%",
        "category": "slurm--",
        "icon": "$(sync)"
      },
      {
        "command": "slurm--.unautoRefreshTask",
        "title": "%slurm--.unautoRefreshTask.title%",
        "category": "slurm--",
        "icon": "$(sync~spin)"
      },
      {
        "command": "slurm--.cancelTask",
        "title": "%slurm--.cancelTask.title%",
        "category": "slurm--",
        "icon": "$(close)"
      },
      {
        "command": "slurm--.cancelAllTasks",
        "title": "%slurm--.cancelAllTasks.title%",
        "category": "slurm--",
        "icon": "$(close-all)"
      },
      {
        "command": "slurm--.cancelSelectedTasks",
        "title": "%slurm--.cancelSelectedTasks.title%",
        "category": "slurm--",
        "icon": "$(close-all)"
      },
      {
        "command": "slurm--.confirmTask",
        "title": "%slurm--.confirmTask.title%",
        "category": "slurm--",
        "icon": "$(check)"
      },
      {
        "command": "slurm--.confirmAllTask",
        "title": "%slurm--.confirmAllTask.title%",
        "shortTitle": "confirm all",
        "category": "slurm--",
        "icon": "$(check-all)"
      },
      {
        "command": "slurm--.openFile",
        "title": "%slurm--.openFile.title%",
        "category": "slurm--",
        "icon": "$(go-to-file)"
      },
      {
        "command": "slurm--.openStdout",
        "title": "%slurm--.openStdout.title%",
        "category": "slurm--",
        "icon": "$(info)"
      },
      {
        "command": "slurm--.openStderr",
        "title": "%slurm--.openStderr.title%",
        "category": "slurm--",
        "icon": "$(question)"
      },
      {
        "command": "slurm--.refreshResources",
        "title": "%slurm--.refreshResources.title%",
        "category": "slurm--",
        "icon": "$(refresh)"
      },
      {
        "command": "slurm--.autoRefreshRes",
        "title": "%slurm--.autoRefreshRes.title%",
        "category": "slurm--",
        "icon": "$(sync)"
      },
      {
        "command": "slurm--.unautoRefreshRes",
        "title": "%slurm--.unautoRefreshRes.title%",
        "category": "slurm--",
        "icon": "$(sync~spin)"
      },
      {
        "command": "slurm--.copyGres",
        "title": "%slurm--.copyGres.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.launchTerminal",
        "title": "%slurm--.launchTerminal.title%",
        "category": "slurm--",
        "icon": "$(terminal)"
      },
      {
        "command": "slurm--.refreshLauncher",
        "title": "%slurm--.refreshLauncher.title%",
        "category": "slurm--",
        "icon": "$(refresh)"
      },
      {
        "command": "slurm--.launchScript",
        "title": "%slurm--.launchScript.title%",
        "category": "slurm--",
        "icon": "$(run)"
      },
      {
        "command": "slurm--.removeScript",
        "title": "%slurm--.removeScript.title%",
        "category": "slurm--",
        "icon": "$(close)"
      },
      {
        "command": "slurm--.deleteArg",
        "title": "%slurm--.deleteArg.title%",
        "category": "slurm--",
        "icon": "$(close)"
      },
      {
        "command": "slurm--.changeArg",
        "title": "%slurm--.changeArg.title%",
        "category": "slurm--",
        "icon": "$(pencil)"
      },
      {
        "command": "slurm--.openConfig",
        "title": "%slurm--.openConfig.title%",
        "category": "slurm--",
        "icon": "$(gear)"
      },
      {
        "command": "slurm--.setTaskSortById",
        "title": "%slurm--.sortById.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedTaskSortById",
        "title": "%slurm--.sortingById.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setTaskSortByName",
        "title": "%slurm--.sortByName.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedTaskSortByName",
        "title": "%slurm--.sortingByName.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setTaskSortAscend",
        "title": "%slurm--.sortAscend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedTaskSortAscend",
        "title": "%slurm--.sortingAscend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setTaskSortDescend",
        "title": "%slurm--.sortDescend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedTaskSortDescend",
        "title": "%slurm--.sortingDescend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setResourceSortByName",
        "title": "%slurm--.sortByName.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedResourceSortByName",
        "title": "%slurm--.sortingByName.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setResourceSortByAvail",
        "title": "%slurm--.sortByAvail.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedResourceSortByAvail",
        "title": "%slurm--.sortingByAvail.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setResourceSortAscend",
        "title": "%slurm--.sortAscend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedResourceSortAscend",
        "title": "%slurm--.sortingAscend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.setResourceSortDescend",
        "title": "%slurm--.sortDescend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.settedResourceSortDescend",
        "title": "%slurm--.sortingDescend.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.showTaskShortcutKey",
        "title": "%slurm--.showShortcutKey.title%",
        "category": "slurm--"
      },
      {
        "command": "slurm--.hideTaskShortcutKey",
        "title": "%slurm--.hideShortcutKey.title%",
        "category": "slurm--"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "slurm--_explorer",
          "title": "slurm--",
          "icon": "assets/icons/task.svg"
        }
      ]
    },
    "views": {
      "slurm--_explorer": [
        {
          "id": "slurm--_tasks_view",
          "name": "%slurm--_tasks_view.name%",
          "icon": "assets/icons/task.svg"
        },
        {
          "id": "slurm--_resource_view",
          "name": "%slurm--_resource_view.name%",
          "icon": "assets/icons/task.svg"
        },
        {
          "id": "slurm--_launcher_view",
          "name": "%slurm--_launcher_view.name%",
          "icon": "assets/icons/task.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "slurm--.refreshUserTasks",
          "when": "view == slurm--_tasks_view",
          "group": "navigation"
        },
        {
          "command": "slurm--.autoRefreshTask",
          "when": "view == slurm--_tasks_view && !autoRefreshingTask",
          "group": "navigation"
        },
        {
          "command": "slurm--.unautoRefreshTask",
          "when": "view == slurm--_tasks_view && autoRefreshingTask",
          "group": "navigation"
        },
        {
          "command": "slurm--.refreshResources",
          "when": "view == slurm--_resource_view",
          "group": "navigation"
        },
        {
          "command": "slurm--.autoRefreshRes",
          "when": "view == slurm--_resource_view && !autoRefreshingRes",
          "group": "navigation"
        },
        {
          "command": "slurm--.unautoRefreshRes",
          "when": "view == slurm--_resource_view && autoRefreshingRes",
          "group": "navigation"
        },
        {
          "command": "slurm--.refreshLauncher",
          "when": "view == slurm--_launcher_view",
          "group": "navigation"
        },
        {
          "command": "slurm--.openConfig",
          "when": "view == slurm--_tasks_view || view == slurm--_resource_view || view == slurm--_launcher_view"
        },
        {
          "command": "slurm--.setTaskSortById",
          "when": "view == slurm--_tasks_view && taskSortKey != id",
          "group": "sortby@1"
        },
        {
          "command": "slurm--.settedTaskSortById",
          "when": "view == slurm--_tasks_view && taskSortKey == id",
          "group": "sortby@1"
        },
        {
          "command": "slurm--.setTaskSortByName",
          "when": "view == slurm--_tasks_view && taskSortKey != name",
          "group": "sortby@2"
        },
        {
          "command": "slurm--.settedTaskSortByName",
          "when": "view == slurm--_tasks_view && taskSortKey == name",
          "group": "sortby@2"
        },
        {
          "command": "slurm--.setTaskSortAscend",
          "when": "view == slurm--_tasks_view && taskSortDirection != ascending",
          "group": "sortDirection@1"
        },
        {
          "command": "slurm--.settedTaskSortAscend",
          "when": "view == slurm--_tasks_view && taskSortDirection == ascending",
          "group": "sortDirection@1"
        },
        {
          "command": "slurm--.setTaskSortDescend",
          "when": "view == slurm--_tasks_view && taskSortDirection != descending",
          "group": "sortDirection@2"
        },
        {
          "command": "slurm--.settedTaskSortDescend",
          "when": "view == slurm--_tasks_view && taskSortDirection == descending",
          "group": "sortDirection@2"
        },
        {
          "command": "slurm--.setResourceSortByName",
          "when": "view == slurm--_resource_view && gresSortKey != name",
          "group": "sortby@1"
        },
        {
          "command": "slurm--.settedResourceSortByName",
          "when": "view == slurm--_resource_view && gresSortKey == name",
          "group": "sortby@1"
        },
        {
          "command": "slurm--.setResourceSortByAvail",
          "when": "view == slurm--_resource_view && gresSortKey != availability",
          "group": "sortby@2"
        },
        {
          "command": "slurm--.settedResourceSortByAvail",
          "when": "view == slurm--_resource_view && gresSortKey == availability",
          "group": "sortby@2"
        },
        {
          "command": "slurm--.setResourceSortAscend",
          "when": "view == slurm--_resource_view && gresSortDirection != ascending",
          "group": "sortDirection@1"
        },
        {
          "command": "slurm--.settedResourceSortAscend",
          "when": "view == slurm--_resource_view && gresSortDirection == ascending",
          "group": "sortDirection@1"
        },
        {
          "command": "slurm--.setResourceSortDescend",
          "when": "view == slurm--_resource_view && gresSortDirection != descending",
          "group": "sortDirection@2"
        },
        {
          "command": "slurm--.settedResourceSortDescend",
          "when": "view == slurm--_resource_view && gresSortDirection == descending",
          "group": "sortDirection@2"
        },
        {
          "command": "slurm--.showTaskShortcutKey",
          "when": "view == slurm--_tasks_view && !taskShowShortcutKey",
          "group": "common@1"
        },
        {
          "command": "slurm--.hideTaskShortcutKey",
          "when": "view == slurm--_tasks_view && taskShowShortcutKey",
          "group": "common@1"
        }
      ],
      "view/item/context": [
        {
          "command": "slurm--.cancelTask",
          "group": "inline@5",
          "when": "view == slurm--_tasks_view && viewItem == taskItem"
        },
        {
          "command": "slurm--.cancelSelectedTasks",
          "when": "view == slurm--_tasks_view && viewItem == taskItem"
        },
        {
          "command": "slurm--.cancelAllTasks",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == taskList"
        },
        {
          "command": "slurm--.confirmTask",
          "group": "inline@5",
          "when": "view == slurm--_tasks_view && viewItem == finishedTaskItem"
        },
        {
          "command": "slurm--.confirmAllTask",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == finishedTaskList"
        },
        {
          "command": "slurm--.openStdout",
          "group": "inline@1",
          "when": "view == slurm--_tasks_view && taskShowShortcutKey && (viewItem == taskItem || viewItem == finishedTaskItem)"
        },
        {
          "command": "slurm--.openStderr",
          "group": "inline@2",
          "when": "view == slurm--_tasks_view && taskShowShortcutKey && (viewItem == taskItem || viewItem == finishedTaskItem)"
        },
        {
          "command": "slurm--.copyGres",
          "when": "view == slurm--_resource_view && viewItem == gresList"
        },
        {
          "command": "slurm--.launchTerminal",
          "group": "inline",
          "when": "view == slurm--_resource_view && viewItem == nodeItem"
        },
        {
          "command": "slurm--.launchScript",
          "group": "inline",
          "when": "view == slurm--_launcher_view && viewItem == scriptItem"
        },
        {
          "command": "slurm--.removeScript",
          "group": "inline",
          "when": "view == slurm--_launcher_view && viewItem == scriptItem"
        },
        {
          "command": "slurm--.deleteArg",
          "group": "inline",
          "when": "view == slurm--_launcher_view && viewItem == argItem"
        },
        {
          "command": "slurm--.changeArg",
          "group": "inline",
          "when": "view == slurm--_launcher_view && viewItem == argItem"
        }
      ]
    },
    "configuration": {
      "title": "slurm--",
      "properties": {
        "slurm--.tasksPanel.refreshInterval(ms)": {
          "type": "integer",
          "default": 2000,
          "markdownDescription": "%config.tasksPanel.refreshInterval(ms).description%"
        },
        "slurm--.resourcesPanel.refreshInterval(ms)": {
          "type": "integer",
          "default": 2000,
          "markdownDescription": "%config.resourcesPanel.refreshInterval(ms).description%"
        },
        "slurm--.commands.user": {
          "type": "string",
          "default": "--me",
          "markdownDescription": "%config.commands.user.description%"
        },
        "slurm--.tasksPanel.sortBy": {
          "type": "string",
          "enum": ["id", "name"],
          "markdownEnumDescriptions": [
            "%config.tasksPanel.sortBy.id.description%",
            "%config.tasksPanel.sortBy.name.description%"
          ],
          "enumItemLabels": ["%config.label.id%", "%config.label.name%"],
          "default": "id",
          "markdownDescription": "%config.tasksPanel.sortBy.description%"
        },
        "slurm--.tasksPanel.sortDirection": {
          "type": "string",
          "enum": ["ascending", "descending"],
          "markdownEnumDescriptions": [
            "%config.sortDirection.ascending.description%",
            "%config.sortDirection.descending.description%"
          ],
          "enumItemLabels": ["%config.label.ascending%", "%config.label.descending%"],
          "default": "ascending",
          "markdownDescription": "%config.tasksPanel.sortDirection.description%"
        },
        "slurm--.tasksPanel.showShortcutKey": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%config.tasksPanel.showShortcutKey.description%"
        },
        "slurm--.resourcesPanel.sortBy": {
          "type": "string",
          "enum": ["name", "availability"],
          "markdownEnumDescriptions": [
            "%config.resourcesPanel.sortBy.name.description%",
            "%config.resourcesPanel.sortBy.availability.description%"
          ],
          "enumItemLabels": ["%config.label.name%", "%config.label.availability%"],
          "default": "name",
          "markdownDescription": "%config.resourcesPanel.sortBy.description%"
        },
        "slurm--.resourcesPanel.sortDirection": {
          "type": "string",
          "enum": ["ascending", "descending"],
          "markdownEnumDescriptions": [
            "%config.sortDirection.ascending.description%",
            "%config.sortDirection.descending.description%"
          ],
          "enumItemLabels": ["%config.label.ascending%", "%config.label.descending%"],
          "default": "ascending",
          "markdownDescription": "%config.resourcesPanel.sortDirection.description%"
        }
      }
    },
    "keybindings": [
      {
        "command": "slurm--.cancelSelectedTasks",
        "key": "delete",
        "when": "focusedView == slurm--_tasks_view"
      },
      {
        "command": "slurm--.copyGres",
        "key": "ctrl+c",
        "when": "focusedView == slurm--_resource_view"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/slurm.injection.json",
        "scopeName": "slurm.injection.shell",
        "injectTo": [
          "source.shell"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.10",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XingYuSSS/slurm--"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
