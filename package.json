{
  "name": "slurm--extension",
  "displayName": "slurm--",
  "description": "",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionKind": [
    "workspace"
  ],
  "publisher": "xy-sss",
  "contributes": {
    "commands": [
      {
        "command": "slurm--.refreshUserTasks",
        "title": "refresh user tasks",
        "shortTitle": "refresh",
        "category": "slurm--",
        "icon": "$(refresh)"
      },
      {
        "command": "slurm--.refreshing",
        "title": "refreshing",
        "shortTitle": "refreshing",
        "icon": "$(refresh)"
      },
      {
        "command": "slurm--.autoRefreshTask",
        "title": "start auto refresh",
        "category": "slurm--",
        "icon": "$(sync)"
      },
      {
        "command": "slurm--.unautoRefreshTask",
        "title": "stop auto refresh",
        "category": "slurm--",
        "icon": "$(sync~spin)"
      },
      {
        "command": "slurm--.cancelTask",
        "title": "cancel task",
        "shortTitle": "cancel",
        "category": "slurm--",
        "icon": "$(close)"
      },
      {
        "command": "slurm--.cancelSelectedTasks",
        "title": "cancel selected tasks",
        "shortTitle": "cancel selected",
        "category": "slurm--",
        "icon": "$(close-all)"
      },
      {
        "command": "slurm--.confirmTask",
        "title": "confirm task",
        "shortTitle": "confirm",
        "category": "slurm--",
        "icon": "$(check)"
      },
      {
        "command": "slurm--.confirmAllTask",
        "title": "confirm all task",
        "shortTitle": "confirm all",
        "category": "slurm--",
        "icon": "$(check-all)"
      },
      {
        "command": "slurm--.openFile",
        "title": "open",
        "shortTitle": "open",
        "category": "slurm--",
        "icon": "$(go-to-file)"
      },
      {
        "command": "slurm--.refreshResources",
        "title": "refresh resources",
        "shortTitle": "refresh",
        "category": "slurm--",
        "icon": "$(sync)"
      },
      {
        "command": "slurm--.autoRefreshRes",
        "title": "start auto refresh",
        "category": "slurm--",
        "icon": "$(sync)"
      },
      {
        "command": "slurm--.unautoRefreshRes",
        "title": "stop auto refresh",
        "category": "slurm--",
        "icon": "$(sync~spin)"
      },
      {
        "command": "slurm--.copyGres",
        "title": "copy",
        "category": "slurm--"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "slurm--_explorer",
          "title": "slurm--",
          "icon": "assets/icons/task.svg"
        }
      ]
    },
    "views": {
      "slurm--_explorer": [
        {
          "id": "slurm--_tasks_view",
          "name": "tasks",
          "icon": "assets/icons/task.svg"
        },
        {
          "id": "slurm--_resource_view",
          "name": "resources",
          "icon": "assets/icons/task.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "slurm--.refreshUserTasks",
          "when": "view == slurm--_tasks_view && !refreshingUserTasks",
          "group": "navigation"
        },
        {
          "command": "slurm--.refreshing",
          "when": "view == slurm--_tasks_view && refreshingUserTasks",
          "group": "navigation"
        },
        {
          "command": "slurm--.autoRefreshTask",
          "when": "view == slurm--_tasks_view && !autoRefreshingTask",
          "group": "navigation"
        },
        {
          "command": "slurm--.unautoRefreshTask",
          "when": "view == slurm--_tasks_view && autoRefreshingTask",
          "group": "navigation"
        },
        {
          "command": "slurm--.autoRefreshRes",
          "when": "view == slurm--_resource_view && !autoRefreshingRes",
          "group": "navigation"
        },
        {
          "command": "slurm--.unautoRefreshRes",
          "when": "view == slurm--_resource_view && autoRefreshingRes",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "slurm--.cancelTask",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == taskItem"
        },
        {
          "command": "slurm--.confirmTask",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == finishedTaskItem"
        },
        {
          "command": "slurm--.confirmAllTask",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == finishedTaskList"
        },
        {
          "command": "slurm--.openFile",
          "group": "inline",
          "when": "view == slurm--_tasks_view && viewItem == openableFile"
        }
      ]
    },
    "configuration": {
      "title": "slurm--",
      "properties": {
        "slurm--.autoRefresh.interval(ms)": {
          "type": "integer",
          "default": 2000,
          "description": "interval of auto refresh in millisecond"
        }
      }
    },
    "keybindings": [
      {
        "command": "slurm--.cancelSelectedTasks",
        "key": "delete",
        "when": "focusedView == slurm--_tasks_view"
      },
      {
        "command": "slurm--.copyGres",
        "key": "ctrl+c",
        "when": "focusedView == slurm--_resource_view"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/slurm.injection.json",
        "scopeName": "slurm.injection.shell",
        "injectTo": [
          "source.shell"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.10",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XingYuSSS/slurm--"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
