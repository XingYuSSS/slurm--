{
  "name": "slurm--",
  "displayName": "slurm--",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "commands": [
      {
        "command": "slurm--.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "slurm--.refreshUserTasks",
        "title": "refresh user tasks",
        "shortTitle": "refresh",
        "category": "slurm--",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "slurm--.cancelTask",
        "title": "cancel task",
        "shortTitle": "cancel",
        "category": "slurm--",
        "icon": "$(close)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "slurm--_explorer",
          "title": "slurm--",
          "icon": "assets/icons/task.svg"
        }
      ]
    },
    "views": {
      "slurm--_explorer": [
        {
          "id": "slurm--_tasks_view",
          "name": "tasks",
          "contextualTitle": "My Custom View Title",
          "location": "workbench.panel"
        },
        {
          "id": "slurm--_config_view",
          "name": "config",
          "contextualTitle": "My Custom View Title",
          "location": "workbench.panel"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "slurm--.refreshUserTasks",
          "when": "view == slurm--_tasks_view",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "slurm--.cancelTask",
          "group": "inline",
          "when": "view == slurm--_tasks_view",
          "arguments": [
            
          ]
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
